<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head'); %>
</head>

<body>
  <header>
    <%- include('../partials/header'); %>
    <script src="/static/show.js"></script>
  </header>
  <div class="containter-fluid">
    <% let i=0 %>
    <% output.forEach((output)=>  {  %>
    <% if ( i==0) { %>
    <div class="row row-cols-3">
      <div class="col-lg-4 col-md-4 col-sm-4 col-4">
        <% } %>
        <span>
          <h6> <%= output.title %> </h6>
        </span>
        <picture>
          <img id="images" data-show="<%= output.link %>" loading="lazy" src="/static/cache/<%= output.webm %>" width="540" onerror="this.onerror=null;this.src='<%= output.image %>';" class="img-fluid img-thumbnail" alt="Show image">
      </picture>
          <% if (i <=3) { i +=1 %>
          <% } %>
          <% if (i < 3) { %>
      </div>
      <div class=" col-lg-4 col-md-4 col-sm-4 col-4">
          <% } %>
          <% if (i==3) { i=0 %>
      </div>
    </div>
    <% } %>
    <% }) %>
  </div>
  </div>
  </div>
  <footer>
    <%- include('../partials/footer'); %>
  </footer>
</body>

</html>