<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head'); %>
</head>

<body>
  <header>
    <%- include('../partials/header'); %>
  </header>
  <div class="containter-fluid">
    <% let i=0 %>
    <% show.forEach((show)=>  {  %>
    <% if ( show.url == test) { %>
    <% if ( i==0) { %>
    <div class="row row-cols-3">
      <div class="col-lg-4 col-md-4 col-sm-4 col-4">
        <% } %>
        <span>
          <h6> <%= show.title %> </h6>
        </span>
        <video controls id="videoPlayer" width="100%" preload="none">
          <source src="<%= show.link %> " type="video/mp4" />
          <track label="English" kind="descriptions" default>
        </video>
        <% if (i <=3) { i +=1 %>
        <% } %>
        <% if (i < 3) { %>
      </div>
      <div class="col-lg-4 col-md-4 col-sm-4 col-4">
        <% } %>
        <% if (i==3) { i=0 %>
      </div>
    </div>
    <% } %>
    <% } %>
    <% }) %>
  </div>
  </div>
  </div>
  <footer>
    <%- include('../partials/footer'); %>
  </footer>
</body>

</html>