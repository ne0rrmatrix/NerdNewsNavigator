<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head'); %>
</head>

<body class="container">
  <header>
    <%- include('../partials/header'); %>
  </header>
  <main>
    <div class="container-fluid">
      <% podcast.forEach((podcast)=>  {  %>
      <h1>
        <%= podcast.title %>
      </h1>
        <video id="video" controls id="videoPlayer" poster="/pages/cache/<%= podcast.webm  %>" width="100%" preload="none">
          <source src="<%= podcast.link %> " type="video/mp4" />
          <track label="English" kind="descriptions" default>
        </video>
      
      <% }) %> 
    </div>
  </main>
  <footer>
    <%- include('../partials/footer'); %>
  </footer>
</body>

</html>